<p id="notice"><%= notice %></p>
<style>
  body  {
  background-image: url('https://cdn1.vectorstock.com/i/1000x1000/53/30/paper-notes-top-view-note-sticker-set-vector-23395330.jpg');
    }
</style>

<h1>Notes</h1>
<%= link_to 'New Note', new_note_path, class:"btn btn-primary"%>

<div class="card">
  <div class="card-header" style="justify-content:center">
    <% @notes.each do |note| %>
    <%= link_to note do %>
      <h2><%= note.title %></h2>
      <hr>
      <%end%>
    <% end %>
  </div>
</div>

<br>


